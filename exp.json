{
  "name": "gqlgram",
  "description": "Instagram clone made with expo",
  "slug": "gqlgram",
  "privacy": "unlisted",
  "sdkVersion": "15.0.0",
  "version": "1.0.0",
  "orientation": "portrait",
  "primaryColor": "#cccccc",
  "icon": "./assets/icons/app-icon.png",
  "notification": {
    "icon": "./assets/icons/notification-icon.png",
    "color": "#000000"
  },
  "loading": {
    "icon": "./assets/icons/loading-icon.png",
    "hideExponentText": true
  },
  "packagerOpts": {
    "assetExts": ["ttf"]
  },
  "ios": {
    "supportsTablet": false
  }
}
